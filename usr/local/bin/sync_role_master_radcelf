set_out() {                                                                     
    set.site 0 SIG:SYNC_OUT:CLK         CLK                                     
    set.site 0 SIG:SYNC_OUT:CLK:DX      d2
    set.site 0 SIG:SYNC_OUT:TRG         TRG                                     
    set.site 0 SIG:SYNC_OUT:TRG:DX      d2                                      
    set.site 0 SIG:SYNC_OUT:SYNC        SYNC                                    
    set.site 0 SIG:SYNC_OUT:SYNC:DX d2                                          
}                                                                               

set_out      
